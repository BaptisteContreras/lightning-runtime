{
  "name": "sflightning/runtime",
  "description": "Lightning runtime",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Baptiste CONTRERAS",
      "homepage": "https://github.com/BaptisteContreras"
    }
  ],
  "require": {
    "symfony/runtime": "^5.3 || ^6.0"
  },
  "require-dev": {
    "illuminate/http": "^8.48",
    "swoole/ide-helper": "^4.6",
    "symfony/http-foundation": "^5.3 || ^6.0",
    "symfony/http-kernel": "^5.4 || ^6.0",
    "symfony/phpunit-bridge": "^5.3"
  },
  "conflict": {
    "ext-swoole": "<4.6.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Sflightning\\Runtime\\": "src/Runtime"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sflightning\\Runtime\\Test\\": "tests/"
    }
  }
}